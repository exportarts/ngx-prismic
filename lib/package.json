{
  "name": "ngx-prismic",
  "version": "0.4.0",
  "description": "This project is intended to provide easier integration of prismic.io into Angular Projects.",
  "author": {
    "name": "Exportarts",
    "email": "info@exportarts.io",
    "url": "https://github.com/exportarts"
  },
  "contributors": [
    {
      "name": "Enzo Volkmann",
      "email": "enzo@exportarts.io",
      "url": "https://github.com/evolkmann"
    }
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/exportarts/ngx-prismic"
  },
  "license": "MIT",
  "keywords": [
    "angular",
    "prismic"
  ],
  "ngPackage": {
    "$schema": "../node_modules/ng-packagr/ng-package.schema.json",
    "src": "./",
    "dest": "../dist/lib",
    "lib": {
      "entryFile": "./public_api.ts"
    },
    "whitelistedNonPeerDependencies": [
      "mkdir-recursive",
      "sitemap"
    ]
  },
  "dependencies": {
    "mkdir-recursive": "^0.4.0",
    "sitemap": "^2.1.0"
  },
  "peerDependencies": {
    "@angular/common": "~7.2.0",
    "@angular/core": "~7.2.0",
    "@angular/http": "^7.2.8",
    "@angular/platform-server": "^7.2.8",
    "@nguniversal/module-map-ngfactory-loader": "^7.1.1",
    "prismic-dom": "^2.1.0",
    "prismic-javascript": "^2.0.1"
  }
}
