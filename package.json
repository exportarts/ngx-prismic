{
  "name": "ngx-prismic",
  "description": "This project is intended to provide easier integration of prismic.io into Angular Projects.",
  "version": "2.1.0",
  "license": "MIT",
  "scripts": {
    "ng": "nx",
    "postinstall": "node ./decorate-angular-cli.js && ngcc --properties es2015 browser module main --first-only --create-ivy-entry-points",
    "nx": "nx",
    "start": "ng serve",
    "build": "nx run-many --all --target build --prod",
    "test": "ng test",
    "test:cov": "nx run-many --all --target test --coverage",
    "lint": "nx workspace-lint && ng lint",
    "e2e": "ng e2e",
    "affected:apps": "nx affected:apps",
    "affected:libs": "nx affected:libs",
    "affected:build": "nx affected:build",
    "affected:e2e": "nx affected:e2e",
    "affected:test": "nx affected:test",
    "affected:lint": "nx affected:lint",
    "affected:dep-graph": "nx affected:dep-graph",
    "affected": "nx affected",
    "format": "nx format:write",
    "format:write": "nx format:write",
    "format:check": "nx format:check",
    "update": "ng update @nrwl/workspace",
    "workspace-schematic": "nx workspace-schematic",
    "dep-graph": "nx dep-graph",
    "help": "nx help",
    "upload-cov": "codecov",
    "changelog": "standard-changelog",
    "publish": "bash tools/scripts/publish.sh"
  },
  "private": true,
  "dependencies": {
    "@nrwl/angular": "9.6.0",
    "@angular/animations": "9.1.12",
    "@angular/common": "9.1.12",
    "@angular/compiler": "9.1.12",
    "@angular/core": "9.1.12",
    "@angular/forms": "9.1.12",
    "@angular/platform-browser": "9.1.12",
    "@angular/platform-browser-dynamic": "9.1.12",
    "@angular/router": "9.1.12",
    "rxjs": "6.6.3",
    "tslib": "1.13.0",
    "zone.js": "0.11.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "0.901.0",
    "@angular-devkit/build-ng-packagr": "0.901.9",
    "@angular/cli": "9.1.0",
    "@angular/compiler-cli": "9.1.12",
    "@angular/language-service": "9.1.12",
    "@nrwl/cypress": "9.6.0",
    "@nrwl/jest": "9.6.0",
    "@nrwl/workspace": "9.6.0",
    "@types/jest": "26.0.4",
    "@types/node": "8.10.63",
    "codecov": "3.7.2",
    "codelyzer": "5.2.2",
    "cypress": "4.12.1",
    "dotenv": "8.2.0",
    "eslint": "6.8.0",
    "jest": "25.5.4",
    "jest-preset-angular": "8.3.1",
    "ng-packagr": "9.1.5",
    "prettier": "2.1.1",
    "prismic-dom": "2.2.3",
    "prismic-javascript": "3.0.1",
    "standard-changelog": "2.0.24",
    "ts-jest": "26.2.0",
    "ts-node": "7.0.1",
    "tslint": "6.1.3",
    "typescript": "3.8.3"
  }
}
